#: bot/ukbot.py:246
#, python-format
msgid ""
"A problem encountered with revision %(revid)d may have influenced the word "
"count for this revision: <nowiki>%(problems)s</nowiki> "
msgstr ""
"Det oppsto et problem med revisjon %(revid)d, som kan ha "
"påvirketordtellingen for denne revisjonen: <nowiki>%(problems)s</nowiki> "

